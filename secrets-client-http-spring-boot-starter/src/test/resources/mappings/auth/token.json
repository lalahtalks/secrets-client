{
  "request": {
    "urlPath": "/oauth/token",
    "method": "POST",
    "bodyPatterns": [
      {
        "equalTo": "grant_type=client_credentials"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "access_token": "abc",
      "token_type": "Bearer",
      "expires_in": 3600,
      "example_parameter": "example_value"
    }
  }
}
